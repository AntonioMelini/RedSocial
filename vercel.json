{
    "version":2,
    "builds":[
        {
            "src":"./api/index-api.js",
            "use":"@vercel/node"
        },
        {
            "src":"./microservices/mysql/index-mysql.js ",
            "use":"@vercel/node"
        },
        {
            "src":"./microservices/post/index-post.js ",
            "use":"@vercel/node"
        }
        
    ],
    "routes":[
        {
            "src":"/api/src/components/auth(.*)",
            "dest":"/api/index-api.js"
        },
        {
            "src":"/api/src/components/user(.*)",
            "dest":"/api/index-api.js"
        },
        {
            "src":"./microservices/post(.*)",
            "dest":"/microservices/post/index-post.js"
        }
]
}